@page
@model AspNetCoreWebAppCodeFirst.Pages.TrucksModel
@{
}

<table class="table">
    @foreach (var car in Model.Trucks)
    {
        <tr>
            <td>@car.Model</td>
            <td>@car.Manufacturer</td>
            <td>@car.Price</td>
            <td>@car.Year</td>
            <td><a href="#" class="btn btn-primary">View</a></td>
        </tr>
    }
</table>